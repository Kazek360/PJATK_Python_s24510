print('Kartoteka')

print("Podaj dane \n" +
      "Wiek:")
wiek = int(input())

print("Wzrost:")
wzrost = int(input())

print("Kolor oczu:")
oczy = str(input())

print("Kolor włosów:")
wlosy = str(input())

print(str(wiek) + " " + str(wzrost) + " " + oczy + " " + wlosy)
