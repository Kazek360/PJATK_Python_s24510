print("Podaj rozmiar tabliczki mnoÅ¼enia:")
rozmiar = int(input())


for i in range(1, rozmiar+1):
    lista = []
    for j in range(1,6):
        wynik = i*j
        lista.append(wynik)
    formatted_list = [f"{num:4d}" for num in lista]
    print(" ".join(formatted_list))
